{
  "name": "search-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "eslint": "eslint .",
    "prettier": "prettier --write .",
    "jest:test": "jest --maxWorkers=50%",
    "jest:coverage": "jest --coverage",
    "ssr": "set NODE_ENV=development&& nodemon --watch ./ --exec ts-node ./server/server.tsx",
    "cy:run": "npm run test:clean && cypress run --e2e && npx nyc report",
    "cy:browser": "cypress open --e2e --browser chrome",
    "cy:coverage": "npx nyc report --reporter=text-summary",
    "test:clean": "npx rimraf coverage && npx rimraf .nyc_output"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "vite-node": "^0.30.1",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/types": "^7.21.3",
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/babel__core": "^7.20.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-jest": "^29.5.0",
    "cypress": "^12.10.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "msw": "^1.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "sort-by": "^0.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-istanbul": "^4.0.1"
  }
}
